{
    "manifest_version": 2,
    "name": "Opening skiper",
    "version": "1.1",
    "description": "Skip Anime Opening for all anime websites",
    "permissions": ["tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["skip.js"],
            "css": ["style.css"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "./pictures/icon-16.png",
        "48": "./pictures/icon-48.png",
        "96": "./pictures/icon-96.png",
        "128": "./pictures/icon-128.png"
    }
}